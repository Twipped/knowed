{
  "name": "handybars",
  "version": "0.3.0",
  "description": "A light-weight templating engine inspired by Handlebars",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "tap -R base --no-coverage tests",
    "cover": "tap -R terse --coverage tests",
    "exe": "babel-node --presets @babel/env",
    "lint": "eslint './{src,tests}/**/*.{js,jsx}' --fix",
    "build": "rollup -c",
    "clean": "rm -rf ./dist ./.nyc_output ./util.js"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "author": "Jocelyn Badgley <joc@twipped.com> (http://twipped.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/node": "~7.8.7",
    "@babel/preset-env": "~7.9.5",
    "@rollup/plugin-node-resolve": "~7.1.3",
    "babel-eslint": "~10.1.0",
    "babel-node": "0.0.1-security",
    "csv-parser": "~3.0.0",
    "eslint": "~6.8.0",
    "eslint-config-twipped": "~3.4.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~4.2.1",
    "js-beautify": "~1.11.0",
    "marky": "~1.2.1",
    "rollup": "~2.7.2",
    "rollup-plugin-banner": "~0.2.1",
    "tap": "~14.10.5",
    "tapsuite": "~2.0.1",
    "through2": "~4.0.2"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "fs-extra": "~9.0.1",
    "stream-chain": "~2.2.4",
    "stream-json": "~1.7.1"
  }
}
