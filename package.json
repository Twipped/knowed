{
  "name": "handybars",
  "version": "0.3.0",
  "description": "A light-weight templating engine inspired by Handlebars",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "tap -R base --no-coverage tests",
    "cover": "tap -R terse --coverage tests",
    "exe": "babel-node --presets @babel/env",
    "lint": "eslint './{src,tests}/**/*.{js,jsx}' --fix",
    "build": "rollup -c",
    "clean": "rm -rf ./dist ./.nyc_output ./util.js"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "author": "Jocelyn Badgley <joc@twipped.com> (http://twipped.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.12.10",
    "@babel/node": "~7.12.10",
    "@babel/preset-env": "~7.12.11",
    "@rollup/plugin-commonjs": "~17.0.0",
    "@rollup/plugin-node-resolve": "~11.1.0",
    "babel-eslint": "~10.1.0",
    "babel-node": "0.0.1-security",
    "csv-parser": "~3.0.0",
    "eslint": "~7.18.0",
    "eslint-config-twipped": "~3.4.0",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~4.2.1",
    "js-beautify": "~1.13.4",
    "marky": "~1.2.1",
    "rollup": "~2.38.0",
    "rollup-plugin-babel": "~4.4.0",
    "rollup-plugin-banner": "~0.2.1",
    "rollup-plugin-exclude-dependencies-from-bundle": "~1.1.17",
    "tap": "~14.11.0",
    "tapsuite": "~2.0.1",
    "through2": "~4.0.2"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "fs-extra": "~9.1.0",
    "stream-chain": "~2.2.4",
    "stream-json": "~1.7.1"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  }
}
